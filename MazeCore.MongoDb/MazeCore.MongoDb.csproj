<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Version>1.0.215</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Authors>Duy Khanh</Authors>
    <Product>MongoDb Provider</Product>
    <Description>Extension MongoDb Provider for .NET Core</Description>
    <PackageIcon>Maze.png</PackageIcon>
    <PackageIconUrl />
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Copyright>@ 2021 - Duy Khanh</Copyright>
    <AssemblyVersion>1.0.215.100</AssemblyVersion>
    <FileVersion>1.0.215.100</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="5.0.0" />
    <PackageReference Include="MongoDB.Driver" Version="2.13.2" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Assets\Maze.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
